{
  "name": "project-template",
  "version": "2.0.0",
  "original-template-version": "2.0.0",
  "description": "",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rmdir /S /Q dist \"docs/typedoc\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "node dist/server",
    "start:dev": "ts-node src/server",
    "test": "nyc --reporter=html --reporter=text mocha --require ts-node/register test/**/*.ts --timeout 5000",
    "test:watch": "mocha --require ts-node/register --watch --watch-files src, test/**/*.ts",
    "typedoc": "typedoc --options ./typedoc.json"
  },
  "keywords": [],
  "author": "Leo Lam",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "inversify": "^5.0.5",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.12.1",
    "mocha": "^8.2.0",
    "mocha-teamcity-reporter": "^3.0.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "sinon": "^9.2.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  }
}
