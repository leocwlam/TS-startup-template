{
  "name": "project-template",
  "version": "2.0.0",
  "original-template-version": "2.0.0",
  "description": "",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rmdir /S /Q dist \"docs/typedoc\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "node dist/server",
    "start:dev": "ts-node src/server",
    "test": "nyc --reporter=html --reporter=text mocha --require ts-node/register test/**/*.ts --timeout 5000",
    "test:watch": "mocha --require ts-node/register --watch --watch-files src, test/**/*.ts",
    "typedoc": "typedoc --options ./typedoc.json",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged"
  },
  "keywords": [],
  "author": "Leo Lam",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.2.0",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^4.29.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "eslint": "^8.13.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "mocha-teamcity-reporter": "^4.1.0",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.1.0",
    "typedoc": "^0.23.9",
    "typescript": "^4.3.5"
  }
}
